import java.util.Scanner;

double sum = 0;
double num = 5. / 6;
System.out.print("Введите е: ");
double e = sc.nextDouble();
int n = 2;

while (Math.abs(num) >= e) {
    sum = sum + num;
    num = Math.pow((double) 1 / 2, n) + Math.pow((double) 1 / 3, n);
