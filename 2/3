import java.util.Scanner;
//Через построение канонического уравнения прямой по двум точка.

System.out.print("Введите координаты первой точки: ");
double x1 = sc.nextDouble(), y1 = sc.nextDouble();
System.out.print("Введите координаты второй точки: ");
double x2 = sc.nextDouble(), y2 = sc.nextDouble();
System.out.print("Введите координаты второй точки: ");
double x3 = sc.nextDouble(), y3 = sc.nextDouble();

if ((x3 - x1) / (x2 - x1) == (y3 - y1) / (y2 - y1)) {
    System.out.println("Принадлежат.");
} else System.out.println("Не принадлежат.");
sc.close();
