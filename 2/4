import java.util.Scanner;
//пройдет ли кирпич через отверстие

System.out.print("Введите размеры отверстия: ");
double a = sc.nextDouble(), b = sc.nextDouble();
System.out.print("Введите размеры кирпича: ");
double x = 3, y = 5, z = 8;
if ((a == x && b == y) || (a == y && b == x) || (a == x && b == z) ||
        (a == z && b == x) || (a == y && b == z) || (a == z && b == y)) {
    System.out.println("Пройдет.");
} else System.out.println("Не пройдет.");
sc.close();
