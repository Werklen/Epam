import java.util.Scanner;
//Подсчет отр, нулевых и полож элементов
int n = sc.nextInt();
double[] arr = new double[n];

for (int i = 0; i < n; i++) {
    arr[i] = i - 3.1;
    System.out.print(arr[i] + " ");
}
System.out.println();

int otr = 0, nul = 0, plus = 0;
for (int i = 0; i < n; i++) {
    if (arr[i] < 0) {
        otr++;
    } else if (arr[i] == 0) {
        nul++;
    } else plus++;
}
System.out.println(otr + " " + nul + " " + plus);
sc.close();
