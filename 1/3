В строке найти количество цифр.

public static void find(String s) {
  int count = 0;
  for (int i = 0; i < 10; i++) {
      for (int j = 0; j < s.length(); j++) {
          if (s.charAt(j) == i+49) {
              count++;
          }
      }
  }
  System.out.println(count);
}

String s = "number7word, iw67578ordHotSummer, word89687Winter, don'tword.";
find(s);
