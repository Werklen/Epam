import java.util.Scanner;
//Найти сумму элементов, кратных н

Scanner sc = new Scanner(System.in);
System.out.print("Введите натуральное число: ");
int n = sc.nextInt();
int[] arr = new int[n];

for (int i = 0; i < n; i++) {
    arr[i] = i + 1;
    System.out.print(arr[i]+" ");
}
System.out.println();

int k = sc.nextInt();
int sum = 0;

for (int i = 0; i < n; i++) {
    if (arr[i] % k == 0) {
        sum += arr[i];
    }
}
System.out.print(sum);
sc.close();
