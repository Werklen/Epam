import java.util.Scanner;
//Сумма элементов, порядковые номера которых простые
int n = sc.nextInt();
double[] arr = new double[n];
boolean yes = true;
double sum = 0;

for (int i = 0; i < n; i++) {
    arr[i] = i - 3.1;
    System.out.print(arr[i] + " ");
    for (int j = 2; j < i / 2 + 1; j++) {
        if (i % j == 0) {
            yes = false;
        }
    }
    if (yes && i != 0) {
        sum += arr[i];
    }
    yes = true;
}
System.out.println();
System.out.println(sum);
sc.close();
