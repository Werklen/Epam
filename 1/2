import java.util.Scanner;
//Заменить все элементы, большие z, этим числом
int n = sc.nextInt();
double[] arr = new double[n];

for (int i = 0; i < n; i++) {
    arr[i] = i + 1.1;
    System.out.print(arr[i] + " ");
}
System.out.println();

double z = sc.nextDouble();

int count=0;
for (int i = 0; i < n; i++) {
    if (arr[i] > z) {
        arr[i] = z;
        count++;
    }
    System.out.print(arr[i] + " ");
}
System.out.println();
System.out.println(count);
sc.close();
